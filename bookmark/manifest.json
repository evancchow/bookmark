{
    "manifest_version": 2,
    "name": "Now and Later",
    "description": "Save pages for later, and view them when you are not busy.",
    "version": "1.1",
    "icons": { "16": "icon_16x16.png",
           "48": "icon_48x48.png",
          "128": "icon_128x128.png" },
    "permissions" : [
        "storage",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon_16x16.png",
        "default_popup": "bookmark.html"
    }
}